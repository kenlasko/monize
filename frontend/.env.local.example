# Frontend Environment Variables
# Copy this file to .env.local and update the values

# Backend URL for server-side communication (rewrites)
# Local dev: http://localhost:3001 (or your backend port)
# Docker: http://backend:3000
# Kubernetes: http://backend-svc:3000
INTERNAL_API_URL=http://localhost:3001

# Public app URL (for OAuth callbacks, sharing links)
PUBLIC_APP_URL=http://localhost:3000

# Log level: error, warn, info (default), debug
# NEXT_PUBLIC_LOG_LEVEL=info
